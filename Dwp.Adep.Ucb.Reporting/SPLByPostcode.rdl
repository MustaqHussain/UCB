<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AdepUcbDS">
      <DataSourceReference>AdepUcbDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>af2f217e-cdc1-498d-9560-2e407b32d0fd</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="IncidentsDataSet">
      <Query>
        <DataSourceName>AdepUcbDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserId">
            <Value>=Parameters!UserID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Postcode">
            <Value>=Parameters!Postcode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- To help prevent deadlocks and NOLOCKS on all SELECTS
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

EXECUTE [SetContextInfo] @UserId

SELECT
	i.IncidentID,
	i.Code, 
	i.IncidentDate,
	c.NINO,
	c.LastName,
	c.FirstName,
	o4.Name AS Region,
	o5.Name AS District,
	s.SiteName,
	( SELECT cm.ControlMeasureDescription + ', ' 
			FROM CustomerControlMeasure ccm
				INNER JOIN ControlMeasure cm ON ccm.ControlMeasureCode = cm.Code 
			WHERE ccm.CustomerCode = c.Code
			FOR XML PATH ('')) as ControlMeasures,
	ic.[Description] AS [IncidentCategoryDescription],
	c.Postcode,
	1 AS [NumberofIncidents]
FROM
	INCIDENT i 
		INNER JOIN Customer c ON i.CustomerCode = c.Code
		INNER JOIN [Site] s ON i.StaffMemberHomeOfficeSiteCode = s.Code
		INNER JOIN [Organisation] o6 ON s.OrganisationCode = o6.Code
		INNER JOIN [OrganisationHierarchy] oh1 ON oh1.OrganisationCode = o6.Code
		INNER JOIN [Organisation] o4 ON oh1.AncestorOrganisationCode = o4.Code AND oh1.HopsBetweenOrgAndAncestor = 2
		INNER JOIN [OrganisationHierarchy] oh2 ON oh2.OrganisationCode = o6.Code
		INNER JOIN [Organisation] o5 ON oh2.AncestorOrganisationCode = o5.Code AND oh2.HopsBetweenOrgAndAncestor = 1
		INNER JOIN IncidentCategory ic ON ic.Code = i.IncidentCategoryCode
WHERE c.Postcode like '%' + LTRIM(RTRIM(@Postcode)) + '%'
ORDER BY c.Postcode,c.LastName,c.FirstName,NINO,Region,SiteName,IncidentID		</CommandText>
      </Query>
