  <DataSets>
    <DataSet Name="IncidentsDataSet">
      <Query>
        <DataSourceName>AdepUcbDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserId">
            <Value>=Parameters!UserID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- To help prevent deadlocks and NOLOCKS on all SELECTS
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

EXECUTE [SetContextInfo] @UserId

DECLARE @IsNomMgr bit
DECLARE @IsDepNomMgr bit
DECLARE @IsBAM bit
DECLARE @IsAdmin bit

SELECT @IsNomMgr = IsNominatedManager,
	@IsDepNomMgr = IsDeputyNominatedManager,
	@IsBAM = IsBusinessAreaManager,
	@IsAdmin= IsAdmin
FROM vwStaffRoles
WHERE Code=@UserId


SELECT 
		CAST(i.Code as VARCHAR(64)) as Code, 
		i.ReviewDate,
		i.IncidentId, 
		i.IncidentDate,
		DATEPART(ww,i.ReviewDate) AS Week,
		DATEADD(dd, - (DATEPART(dw,i.ReviewDate)-1),i.ReviewDate) AS WeekStart,
		DATEADD(dd, 7 - (DATEPART(dw,i.ReviewDate)),i.ReviewDate) AS WeekEnd,
		s.SiteName,
		i.IncidentStatus,
		i.CurrentOwnerStaffCode,
		sto.FirstName + ' ' + sto.LastName as CurrentOwner,
		c.LastName, 
		c.NINO,
		ic.Description,
		st.FirstName as NomMgrFirstName,
		st.LastName  as NomMgrLastName,
		( SELECT cm.ControlMeasureDescription + ', ' 
			FROM CustomerControlMeasure ccm
				INNER JOIN ControlMeasure cm ON ccm.ControlMeasureCode = cm.Code 
			WHERE ccm.CustomerCode = c.Code
			FOR XML PATH ('')) as ControlMeasures
FROM Incident i
	LEFT JOIN Customer c ON i.CustomerCode = c.Code 
	INNER JOIN Site s ON i.StaffMemberHomeOfficeSiteCode = s.Code 
	INNER JOIN Site nms ON i.SiteCode = nms.Code 
	INNER JOIN IncidentCategory ic ON i.IncidentCategoryCode = ic.Code
	INNER JOIN [SiteStaff] ss on ss.SiteCode = nms.Code AND ss.Responsibility = 'Nominated Manager'
	INNER JOIN [Staff] st on st.Code = ss.StaffCode AND st.Code = @UserId
	LEFT JOIN [Staff] sto on i.CurrentOwnerStaffCode = sto.Code
WHERE i.IncidentStatus IN ('Live')
	AND ReviewDate &gt; DATEADD("m", 1, GETDATE())
	AND ReviewDate &lt; DATEADD("yy", 1, GETDATE())
	AND @IsNomMgr = 1
UNION

SELECT 
		CAST(i.Code as VARCHAR(64)) as Code, 
		i.ReviewDate,
		i.IncidentId, 
		i.IncidentDate,
		DATEPART(ww,i.ReviewDate) AS Week,
		DATEADD(dd, - (DATEPART(dw,i.ReviewDate)-1),i.ReviewDate) AS WeekStart,
		DATEADD(dd, 7 - (DATEPART(dw,i.ReviewDate)),i.ReviewDate) AS WeekEnd,
		s.SiteName,
		i.IncidentStatus,
		i.CurrentOwnerStaffCode,
		sto.FirstName + ' ' + sto.LastName as CurrentOwner,
		c.LastName, 
		c.NINO,
		ic.Description,
		st.FirstName as NomMgrFirstName,
		st.LastName  as NomMgrLastName,
		( SELECT cm.ControlMeasureDescription + ', ' 
			FROM CustomerControlMeasure ccm
				INNER JOIN ControlMeasure cm ON ccm.ControlMeasureCode = cm.Code 
			WHERE ccm.CustomerCode = c.Code
			FOR XML PATH ('')) as ControlMeasures
FROM Incident i
	LEFT JOIN Customer c ON i.CustomerCode = c.Code 
	INNER JOIN Site s ON i.StaffMemberHomeOfficeSiteCode = s.Code 
	INNER JOIN Site nms ON i.SiteCode = nms.Code 
	INNER JOIN IncidentCategory ic ON i.IncidentCategoryCode = ic.Code
	INNER JOIN [SiteStaff] ss on ss.SiteCode = nms.Code AND ss.Responsibility = 'Nominated Manager'
	INNER JOIN [Staff] st on st.Code = ss.StaffCode
	INNER JOIN [StaffOrganisation] so ON so.staffCode = @UserId
	INNER JOIN [Organisation] o ON o.Code = so.OrganisationCode
	INNER JOIN [OrganisationType] ot ON o.OrganisationTypeCode = ot.Code AND ot.LevelNumber = 3 
	INNER JOIN [OrganisationHierarchy] oh1 ON s.OrganisationCode = oh1.OrganisationCode
	INNER JOIN [Organisation] o3 ON oh1.AncestorOrganisationCode = o3.Code AND HopsBetweenOrgAndAncestor = 3 AND o3.Code = so.OrganisationCode
	LEFT JOIN [Staff] sto on i.CurrentOwnerStaffCode = sto.Code
WHERE i.IncidentStatus IN ('Live')
	AND ReviewDate &gt; DATEADD("m", 1, GETDATE())
	AND ReviewDate &lt; DATEADD("yy", 1, GETDATE())
	AND @IsBAM = 1
UNION
SELECT 
		CAST(i.Code as VARCHAR(64)) as Code, 
		i.ReviewDate,
		i.IncidentId,
		i.IncidentDate,
		DATEPART(ww,i.ReviewDate) AS Week,
		DATEADD(dd, - (DATEPART(dw,i.ReviewDate)-1),i.ReviewDate) AS WeekStart,
		DATEADD(dd, 7 - (DATEPART(dw,i.ReviewDate)),i.ReviewDate) AS WeekEnd,
		s.SiteName,
		i.IncidentStatus,
		i.CurrentOwnerStaffCode,
		sto.FirstName + ' ' + sto.LastName as CurrentOwner,
		c.LastName, 
		c.NINO,
		ic.Description,
		st.FirstName as NomMgrFirstName,
		st.LastName  as NomMgrLastName,
		( SELECT cm.ControlMeasureDescription + ', ' 
			FROM CustomerControlMeasure ccm
				INNER JOIN ControlMeasure cm ON ccm.ControlMeasureCode = cm.Code 
			WHERE ccm.CustomerCode = c.Code
			FOR XML PATH ('')) as ControlMeasures
FROM Incident i
	LEFT JOIN Customer c ON i.CustomerCode = c.Code 
	INNER JOIN Site s ON i.StaffMemberHomeOfficeSiteCode = s.Code 
	INNER JOIN Site nms ON i.SiteCode = nms.Code 
	INNER JOIN IncidentCategory ic ON i.IncidentCategoryCode = ic.Code
	INNER JOIN [SiteStaff] ss on ss.SiteCode = nms.Code AND ss.Responsibility = 'Nominated Manager'
	INNER JOIN [Staff] st on st.Code = ss.StaffCode
	LEFT JOIN [Staff] sto on i.CurrentOwnerStaffCode = sto.Code
WHERE i.IncidentStatus IN ('Live')
	AND ReviewDate &gt; DATEADD("m", 1, GETDATE())
	AND ReviewDate &lt; DATEADD("yy", 1, GETDATE())
	AND @IsAdmin = 1
 </CommandText>
