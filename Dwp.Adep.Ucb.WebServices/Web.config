    <sources>
      <source name="System.ServiceModel"
   switchValue="Information, ActivityTracing">
        <listeners>
          <add name="log"
     type="System.Diagnostics.XmlWriterTraceListener"
     initializeData="c:\\logs\\ServicesTrace.svclog" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>-->
  
</configuration>
